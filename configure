  'htdig/Makefile' ) CONFIG_FILES="$CONFIG_FILES htdig/Makefile" ;;
  'httools/Makefile' ) CONFIG_FILES="$CONFIG_FILES httools/Makefile" ;;
  'htfuzzy/Makefile' ) CONFIG_FILES="$CONFIG_FILES htfuzzy/Makefile" ;;
  'htsearch/Makefile' ) CONFIG_FILES="$CONFIG_FILES htsearch/Makefile" ;;
  'installdir/Makefile' ) CONFIG_FILES="$CONFIG_FILES installdir/Makefile" ;;
  'include/Makefile' ) CONFIG_FILES="$CONFIG_FILES include/Makefile" ;;
  'htdoc/Makefile' ) CONFIG_FILES="$CONFIG_FILES htdoc/Makefile" ;;
  'test/Makefile' ) CONFIG_FILES="$CONFIG_FILES test/Makefile" ;;
  'test/test_functions' ) CONFIG_FILES="$CONFIG_FILES test/test_functions" ;;
  'default-1' ) CONFIG_COMMANDS="$CONFIG_COMMANDS default-1" ;;
  'include/htconfig.h' ) CONFIG_HEADERS="$CONFIG_HEADERS include/htconfig.h" ;;

  # This is an error.
  -*) echo "$me: unrecognized option: $1
Try \`$me --help' for more information."; exit 1 ;;
  *) echo "$me: invalid argument: $1"; exit 1 ;;
  esac
  shift
done

