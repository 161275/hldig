. ./test_functions --start-apache

verbose=$1

./testnet -U http://localhost:7400/set3/nph-hang.cgi $verbose
if test $? = 1
then
	exit 0
else
	fail "Could not fetch URL"
fi

. ./test-functions --stop-apache
