bench:
	rm -f $(BASE) $(BASE)_weakcmpr
	$(TIMEV) $(top_builddir)/test/dbbench $(CACHESIZE) $(PAGESIZE) $(CMPR) $(WORDS) $(LOOP) -B $(BASE) $(NWORDS)
	ls -l $(BASE)
