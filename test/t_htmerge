. ./test_functions

$htdig "$@" -t -i -c $testdir/conf/htdig.conf
$htdig "$@" -t -i -c $testdir/conf/htdig.conf2

$htmerge "$@" -c $testdir/conf/htdig.conf -m $testdir/conf/htdig.conf2
