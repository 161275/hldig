all clean install:
	cd dist ; if [ -f Makefile ] ; then $(MAKE) $@ ; fi

distclean:
	cd dist ; if [ -f Makefile ] ; then $(MAKE) $@ ; fi
	rm -f Makefile

dvi dist installcheck check:

distdir: distclean
	find * \( ! -name '*~' -a ! -name '.#*' -a ! -name '#*#' -a ! -name core \) -print | egrep -v 'CVS|RCS' | cpio -pdm $(distdir)
